<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/objectionConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.rgp.asks.views.TextInputLayout
        android:id="@+id/objectionTextInputLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        app:errorMessage="@string/objection_dialog_error_empty_objection"
        app:label="@string/hint_objection_description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:numberOfLines="6" />

</androidx.constraintlayout.widget.ConstraintLayout>